<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:common="clr-namespace:CashManager_MVVM.Features.Common"
                    xmlns:tags="clr-namespace:CashManager_MVVM.Features.Tags"
                    xmlns:categories="clr-namespace:CashManager_MVVM.Features.Categories"
                    xmlns:stocks="clr-namespace:CashManager_MVVM.Features.Stocks"
                    xmlns:transactionTypes="clr-namespace:CashManager_MVVM.Features.TransactionTypes"
                    xmlns:transactions="clr-namespace:CashManager_MVVM.Features.Transactions"
                    xmlns:parsers="clr-namespace:CashManager_MVVM.Features.Parsers"
                    xmlns:search="clr-namespace:CashManager_MVVM.Features.Search"
                    xmlns:plots="clr-namespace:CashManager_MVVM.Features.Plots"
                    xmlns:balance="clr-namespace:CashManager_MVVM.Features.Balance"
                    xmlns:massReplacer="clr-namespace:CashManager_MVVM.Features.MassReplacer"
                    xmlns:positions="clr-namespace:CashManager_MVVM.Features.Transactions.Positions"
                    xmlns:summary="clr-namespace:CashManager_MVVM.Features.Summary">

    <DataTemplate DataType="{x:Type search:SearchViewModel}">
        <search:SearchView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type massReplacer:MassReplacerViewModel}">
        <massReplacer:MassReplacerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type transactions:TransactionListViewModel}">
        <transactions:TransactionListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type positions:PositionListViewModel}">
        <positions:PositionListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type transactions:TransactionViewModel}">
        <transactions:TransactionView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type transactionTypes:TransactionTypesViewModel}">
        <transactionTypes:TransactionTypesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type parsers:ParserViewModel}">
        <parsers:ParserView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type stocks:StocksViewModel}">
        <stocks:StocksView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type stocks:StockSummaryViewModel}">
        <stocks:StockSummaryView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type categories:CategoryManagerViewModel}">
        <categories:CategoryManagerView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type tags:TagManagerViewModel}">
        <tags:TagManagerView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type common:MultiComboBoxViewModel}">
        <common:MultiComboBox/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type plots:WealthViewModel}">
        <plots:WealthView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type plots:CategoriesPlotViewModel}">
        <plots:CategoriesPlotView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type balance:CustomBalanceViewModel}">
        <balance:CustomBalanceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type categories:CategoryPickerViewModel}">
        <categories:CategoryPickerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type common:ExtendedDatePickerViewModel}">
        <common:ExtendedDatePicker />
    </DataTemplate>
    <DataTemplate DataType="{x:Type summary:SummaryViewModel}">
        <summary:SummaryView />
    </DataTemplate>

</ResourceDictionary>